@model IEnumerable<RealEstateMVC.Models.Property>

@{
    ViewData["Title"] = "Home";
}

@Html.Partial("Partials/_Header")
@Html.Partial("Partials/_Hero")
@Html.Partial("Partials/_About")
@Html.Partial("Partials/_Service")
@await Html.PartialAsync("Partials/_Property", Model)
@Html.Partial("Partials/_Blog")
@Html.Partial("Partials/_Features")
@Html.Partial("Partials/_Cta")
@await Html.PartialAsync("Partials/_ContactForm", new RealEstateMVC.Models.Message()) 
@Html.Partial("Partials/_Footer")

@section Scripts {
    <script src="~/js/script.js"></script>
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
}
